{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"indent": [true, "tabs"],
		"curly": [true, "ignore-same-line"],
		"quotemark": [true, "single", "avoid-escape", "jsx-double"],
		"object-literal-sort-keys": false,
		"space-before-function-paren": [true, "named"],
		"trailing-comma": [
			true,
			{
				"esSpecCompliant": true,
				"single": "never",
				"multiline": {
					"objects": "always",
					"arrays": "always",
					"functions": "never"
				}
			}
		],
		"whitespace": [
			true,
			"check-branch", "check-decl", "check-operator", "check-module",
			"check-separator", "check-rest-spread", "check-type", "check-type-operator",
			"check-preblock"
		],
		"array-type": [true, "array"],
		"max-line-length": [true, 110],
		"only-arrow-functions": [true, "allow-named-functions"],
		"interface-name": [true, "never-prefix"],
		"ordered-imports": [false]
	},
	"rulesDirectory": []
}
